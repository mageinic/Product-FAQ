<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <mageinic>
            <general>
                <faq_enable>1</faq_enable>
                <faq_questions_display>5</faq_questions_display>
                <footer_link_display>1</footer_link_display>
            </general>
            <slider_setting>
                <enable_slider>1</enable_slider>
                <slider_title>FAQ Category</slider_title>
                <slider_speed>400</slider_speed>
                <navbar>0</navbar>
                <slide_to_show>1</slide_to_show>
                <slide_to_scroll>1</slide_to_scroll>
                <infinite_looping>1</infinite_looping>
                <autoplay_slider>0</autoplay_slider>
                <autoplay_slider_speed>2000</autoplay_slider_speed>
                <dots_slider>0</dots_slider>
                <arrows_slider>0</arrows_slider>
                <breakpoints>
                    <item1>
                        <breakpoint>1024</breakpoint>
                        <slides_to_show>1</slides_to_show>
                        <slides_to_scroll>1</slides_to_scroll>
                        <dots>true</dots>
                    </item1>
                    <item2>
                        <breakpoint>992</breakpoint>
                        <slides_to_show>1</slides_to_show>
                        <slides_to_scroll>1</slides_to_scroll>
                        <dots>true</dots>
                    </item2>
                    <item3>
                        <breakpoint>480</breakpoint>
                        <slides_to_show>1</slides_to_show>
                        <slides_to_scroll>1</slides_to_scroll>
                        <dots>true</dots>
                    </item3>
                </breakpoints>
            </slider_setting>
        </mageinic>
    </default>
</config>
